module completions/PP-esv

imports
  completions/common/Identifiers-esv
  completions/terms/Terms-esv

completions
  completion template PropPattern : "Pattern : Term" =
    <Pattern:Pattern> " : " <Term:Term>  
  completion template PropPattern : "Pattern has Id Term" =
    <Pattern:Pattern> " has " <Id:Id> " " <Term:Term>  
  completion template PropPattern : "Pattern :-" =
    <Pattern:Pattern> " :-"  