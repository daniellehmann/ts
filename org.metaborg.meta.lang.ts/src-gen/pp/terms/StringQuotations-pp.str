module pp/terms/StringQuotations-pp

imports
  libstratego-gpp
  runtime/tmpl/pp
  signatures/terms/StringQuotations-sig


imports
  signatures/terms/Terms-sig


imports
  pp/terms/Terms-pp


strategies
  prettyprint-example =
    prettyprint-PPTerm

  prettyprint-PPTerm :
    t1__ -> [ H(
                [SOpt(HS(), "0")]
              , [t1__']
              )
            ]
    where not(is-PPTerm)
    where t1__' := <pp-one-Z(prettyprint-StringQuotation)> t1__

  is-PPTerm =
    fail

